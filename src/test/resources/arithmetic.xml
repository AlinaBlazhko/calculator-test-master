<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite" parallel="tests" thread-count="10">
    <test name="Test classes for arithmetic methods">
        <groups>
            <run>
                <include name="arithmetic"/>
            </run>
        </groups>
        <classes>
            <class name="tests.testng.DivDoubleTest"/>
            <class name="tests.testng.DivLongTest"/>
            <class name="tests.testng.MultDoubleTest"/>
            <class name="tests.testng.MultLongTest"/>
            <class name="tests.testng.PowTest"/>
            <class name="tests.testng.SqrtTest"/>
            <class name="tests.testng.SubDoubleTest"/>
            <class name="tests.testng.SubLongTest"/>
            <class name="tests.testng.SumDoubleTest"/>
            <class name="tests.testng.SubLongTest"/>
        </classes>
    </test>
</suite>